{
  "expo": {
    "name": "GraficasGFCARTS",
    "slug": "graficasgfcarts",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scripts": {
      "start": "expo start",
      "web": "npx expo export:web"
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.1",
    "@hookform/resolvers": "^3.9.0",
    "axios": "^1.7.2",
    "expo": "~51.0.24",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.1",
    "react-icons": "^5.2.1",
    "react-native": "0.74.2",
    "react-native-web": "~0.19.10",
    "react-router-dom": "^6.26.0",
    "react-router-native": "^6.0.0",
    "react-scripts": "^5.0.1",
    "sweetalert2": "^11.12.3",
    "yup": "^1.4.0"
  }
}
